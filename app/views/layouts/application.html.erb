
<!DOCTYPE html>
<html>
<head>
  <title>Portf</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <nav class="navbar navbar-inverse" role="navigation">
    <div class="container-fluid"> 
    <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-logo">
        <%= image_tag 'laid-cat.png', class: 'img-responsive'%>
      </div>    
      <a class="navbar-brand" href="#"> W e b - D e v e l o p m e n t </a> 
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">   
        <ul class="nav navbar-nav col-sm-5 navbar-right">
          <%= link_to 'About', '#', :class => 'btn btn-warning btn-lg' %>
          <%= link_to 'Work', '#', :class => 'btn btn-warning btn-lg' %>
          <%= link_to 'Drop a line', '#', :class => 'btn btn-warning btn-lg' %>
        </ul>    
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
</header>






<%= yield %>

</body>
</html>
